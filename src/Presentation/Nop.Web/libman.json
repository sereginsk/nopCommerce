{
  "version": "1.0",
  "defaultProvider": "jsdelivr",
  "libraries": [
    {
      "library": "cldrjs@0.5.1",
      "destination": "wwwroot/lib/cldr"
    },
    {
      "library": "cldr-data@36.0.0",
      "destination": "wwwroot/lib/cldr-data"
      // You need add following option to cldr-data/package.json after each library restore 
      // "peerDependencies": {"cldr-data": ">=26"}
    },
    {
      "library": "globalize@1.4.2",
      "destination": "wwwroot/lib/globalize"
    }
  ]
}